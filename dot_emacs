;; -*- lexical-binding: t -*-

(require 'package)
(add-to-list 'package-archives '("melpa" . "http://melpa.org/packages/") t)
(package-initialize)

(setq package-selected-packages
      '(
        cider
        clojure-mode
        clojure-mode-extra-font-locking
        evil
        evil-commentary
        ido-vertical-mode
        key-chord
        markdown-mode
        spacemacs-theme
        yaml-mode
        ))

(add-to-list 'load-path "~/bin/emacs")
(require 'cjb-entry-point)
