;; -*- lexical-binding: t -*-

(require 'package)
(add-to-list 'package-archives '("melpa" . "http://melpa.org/packages/") t)
(package-initialize)

(setq package-selected-packages
      '(
        cider
        clojure-mode
        company
        company-lsp
        enh-ruby-mode
        evil
        evil-commentary
        ido-vertical-mode
        inf-ruby
        key-chord
        lsp-mode
        magit
        markdown-mode
        material-theme
        night-owl-theme
        projectile
        swift-mode
        yaml-mode
        ))

(add-to-list 'load-path "~/bin/emacs")
(require 'cjb-entry-point)
